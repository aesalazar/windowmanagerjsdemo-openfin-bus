{
  "name": "windowmanagerjsdemo-openfin-bus-javascript",
  "version": "0.0.1",
  "description": "Demonstrates using windowmanagerjs with OpenFins inter-application message bus.",
  "main": "app.js",
  "scripts": {
    "test-server": "mocha \"server/**/*.test.js\"",
    "start-server": "node server.js",
    "test-js": "mocha-webpack --webpack-config webpack.test.js \"src-javascript/**/*.test.js\"",
    "build-js": "webpack --config webpack.javascript.js",
    "openfin-js": "node_modules/.bin/openfin --launch --config http://localhost:5000/app-javascript.json"
  },
  "keywords": [
    "WindowManagerJS",
    "Openfin",
    "Java",
    "WPF",
    ".NET",
    "Message Bus",
    "Inter-application"
  ],
  "author": "Ernie Salazar",
  "company": "Eikos Partners, LLC",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "chai": "^4.1.2",
    "css-loader": "^0.28.9",
    "express": "^4.16.2",
    "extract-text-webpack-plugin": "^3.0.2",
    "jshint": "^2.9.5",
    "jshint-loader": "^0.8.4",
    "mocha": "^5.0.0",
    "mocha-loader": "^1.1.2",
    "mocha-webpack": "^1.0.1",
    "openfin-cli": "^1.1.5",
    "sinon": "^4.2.0",
    "webpack": "^3.10.0",
    "webpack-dev-middleware": "^2.0.4",
    "webpack-hot-middleware": "^2.21.0",
    "webpack-node-externals": "^1.6.0"
  },
  "dependencies": {
    "ag-grid": "^15.0.0",
    "lodash": "^4.17.4",
    "ws": "^4.0.0"
  }
}
